{
  "manifest_version": 2,

  "name": "PokeBack",
  "description": "This program will automatically poke your facebook friends back.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "tabs",
    "storage"
  ],
  "content_scripts" : [{
       "matches" : ["https://www.facebook.com/pokes/*"],
       "js" : ["jquery-1.12.2.min.js", "poke.js", "click.js"]
   }]
}
